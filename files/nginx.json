{
    "agent": {
      "metrics_collection_interval": 60,
      "run_as_user": "root"
    },
    "logs": {
      "logs_collected": {
        "files": {
          "collect_list": [
            {
              "file_path": "/var/log/nginx/access.log",
              "log_group_name": "accessLogs",
              "log_stream_name": "accessStream"
            },
            {
              "file_path": "/var/log/nginx/error.log",
              "log_group_name": "errorLogs",
              "log_stream_name": "errorStream"
            }
          ]
        }
      }
    }
  }